95,"action":{"library":"H5P.Shape 1.0","params":{"shape":{"fillColor":"#fff","borderWidth":0,"borderStyle":"solid","borderColor":"#000","borderRadius":0},"type":"rectangle","line":{"borderColor":"#000000","borderWidth":"1","borderStyle":"solid"}},"metadata":{"contentType":"Shapes"},"subContentId":"d9eafda3-2e28-414f-bc8b-90da19998b0d"},"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false},{"x":1.9342359767891684,"y":11.450754435100242,"width":40.59787234042553,"height":72.59694557903705,"action":{"library":"H5P.AdvancedText 1.1","params":{"text":"<p><span style=\"font-size:1.5em\">The male flower has peduncle or stalk about 2-3 cm while the female flower has 3.5 cm long peduncle with small fruit. Both flowers are borne in the same plant. Flowering starts 45-55 days from sowing depending on the variety and could last up to 6 months depending on the cropping pattern.</span></p>\n\n<p> </p>\n"},"metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"32f0a3bd-af0f-4d75-8d9b-ea062ccc97a8"},"alwaysDisplayComments":false,"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false,"solution":""},{"x":44.48742746615087,"y":0,"width":55.51257253384913,"height":100,"action":{"library":"H5P.Image 1.1","params":{"contentName":"Image","file":{"path":"images/image-2XC3wUI6.png","mime":"image/png","copyright":{"license":"U"},"width":1748,"height":1240},"alt":"Flower of Bitter Gourd"},"subContentId":"b187bad7-6d2d-490d-a3f2-fb00340ea615","metadata":{"contentType":"Image","license":"U","title":"Untitled Image","authors":[],"changes":[]}},"alwaysDisplayComments":false,"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false,"solution":""}],"keywords":[],"slideBackgroundSelector":{}}],"keywordListEnabled":true,"globalBackgroundSelector":{},"keywordListAlwaysShow":false