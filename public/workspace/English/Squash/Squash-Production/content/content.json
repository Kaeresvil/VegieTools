isplayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false},{"x":41.45936981757877,"y":22.930820988908035,"width":53.06799336650083,"height":58.96496825719208,"action":{"library":"H5P.AdvancedText 1.1","params":{"text":"<h3><span style=\"font-size:1.25em\">This crop is used in a wide variety of dishes, from vegetable preparations to pastries and dessert.</span></h3>\n"},"metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"ee6c097c-04fd-435f-b873-3d492fb378c3"},"alwaysDisplayComments":false,"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false,"solution":""}],"keywords":[],"slideBackgroundSelector":{}},{"elements":[{"x":46.434494195688224,"y":0,"width":53.58358208955223,"height":100,"action":{"library":"H5P.Image 1.1","params":{"contentName":"Image","file":{"path":"images/image-g4bTwy5X.png","mime":"image/png","copyright":{"license":"U"},"width":1748,"height":1240},"alt":"Squash"},"subContentId":"428710e9-8343-41ae-9781-da1c50d476b3","metadata":{"contentType":"Image","license":"U","title":"Untitled Image","authors":[],"changes":[]}},"alwaysDisplayComments":false,"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false,"solution":""},{"x":0,"y":0,"width":48.09286898839137,"height":100,"action":{"library":"H5P.Shape 1.0","params":{"shape":{"fillColor":"#fff","borderWidth":0,"borderStyle":"solid","borderColor":"#000","borderRadius":0},"type":"rectangle","line":{"borderColor":"#000000","borderWidth":"1","borderStyle":"solid"}},"metadata":{"contentType":"Shapes"},"subContentId":"07d892c2-8bd0-4153-8f1d-6f4630a1c1b7"},"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false},{"x":1.658374792703151,"y":9.8172741150341,"width":43.117744610281925,"height":81.89054791558837,"action":{"library":"H5P.AdvancedText 1.1","params":{"text":"<h3><span style=\"font-size:1.125em\">Like other cucurbits, squash is recognized as an important source of vitamins and minerals. It is usually grown in home gardens and in commercial scale for its fruits, young shoots, flowers, and seeds. Squash can also be used as an intercrop with corn, sugarcane, and coconuts.</span></h3>\n"},"metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"062517fe-9997-45df-8074-858863173dc0"},"alwaysDisplayComments":false,"backgroundOpacity":0,"displayAsButton":false,"buttonSize":"big","goToSlideType":"specified","invisible":false,"solution":""}],"keywords":[],"slideBackgroundSelector":{}}],"keywordListEnabled":true,"globalBackgroundSelector":{},"keywordListAlwaysShow":false,"keywordListAutoHide":false,"keywordListOpacity":90},"override":{"activeSurface":false,"hideSummarySlide":false,"summarySlideSolutionButton":true,"summarySlideRetryButton":true,"enablePrintButton":false,"social":{"showFacebookShare":false,"facebookShare":{"url":"@currentpageurl","quote":"I scored @score out of @maxScore on a task at @currentpageurl."},"showTwitterShare":false,"twitterShare":{"statement":"I scored @score out of @maxScore on a task at @currentpageurl.","url":"@currentpageurl","hashtags":"h5p, course"},"showGoogleShare":false,"googleShareUrl":"@currentpageurl"}},"l10n":{"slide":"Slide","score":"Score","yourScore":"Your Score","maxScore":"Max Score","total":"Total","totalScore":"Total Score","showSolutions":"Show solutions","retry":"Retry","exportAnswers":"Export text","hideKeywords":"Hide sidebar navigation menu","showKeywords":"Show sidebar navigation menu","fullscreen":"Fullscreen","exitFullscreen":"Exit fullscreen","prevSlide":"Previous slide","nextSlide":"Next slide","currentSlide":"Current slide","lastSlide":"Last slide","solutionModeTitle":"Exit solution mode","solutionModeText":"Solution Mode","summaryMultipleTaskText":"Multiple tasks","scoreMessage":"You achieved:","shareFacebook":"Share on Facebook","shareTwitter":"Share on Twitter","shareGoogle":"Share on Google+","summary":"Summary","solutionsButtonTitle":"Show comments","printTitle":"Print","printIngress":"How would you like to print this presentation?","printAllSlides":"Print all slides","printCurrentSlide":"Print current slide","noTitle":"No title","accessibilitySlideNavigationExplanation":"Use left and right arrow to change slide in that direction whenever canvas is selected.","accessibilityCanvasLabel":"Presentation canvas. Use left and right arrow to move between slides.","containsNotCompleted":"@slideName contains not completed interaction","containsCompleted":"@slideName contains completed interaction","slideCount":"Slide @index of @total","containsOnlyCorrect":"@slideName only has correct answers","containsIncorrectAnswers":"@slideName has incorrect answers","shareResult":"Share Result","accessibilityTotalScore":"You got @score of @maxScore points in total","accessibilityEnteredFullscreen":"Entered fullscreen","accessibilityExitedFullscreen":"Exited fullscreen","confirmDialogHeader":"Submit your answers","confirmDialogText":"This will submit your results, do you want to continue?","confirmDialogConfirmText":"Submit and see results"}},"library":"H5P.CoursePresentation 1.22","metadata":{"contentType":"Course Presentation","license":"U","title":"Squash Presentation","authors":[],"changes":[],"extraTitle":"Squash Presentation"},"subContentId":"38dbee6d-7f6d-423d-8f63-fa6830dc62b3"},"useSeparator":"disabled"},{"content":{"params":{"text":"<p> </p>\n\n<h3><span style=\"font-size:1em\">Nutrient Composition</span></h3>\n\n<p>Squash is one of the vegetables with high nutritional value. It is a rich source of the following:</p>\n\n<ul>\n\t<li>\n\t<p>Vitamins and minerals such as Vitamin A, C, B1, B6 and B3. The amount of vitamin A content of squash is comparable to the degree of its yellow color. </p>\n\t</li>\n\t<li>\n\t<p>Phosphorous, calcium and iron. Phytochemicals such as coumarins and flavonoids are also present in squash.</p>\n\t</li>\n\t<li>\n\t<p>Beta-carotene which is a known anti-oxidant and anti-inflammatory. It is also among the top three food sources of health-supportive carotenoids such as lutein, zeaxanthin and betacryptoxanthin. These antioxidants are especially helpful in the protection of the eyes, and against age-related macular degeneration and cataracts. </p>\n\t</li>\n\t<li>\n\t<p>Valuable antioxidant nutrients are also found in skin and seeds of squash. Since the skin of this food is particularly antioxidant-rich, it makes the skin intact.</p>\n\t</li>\n</ul>\n"},"library":"H5P.AdvancedText 1.1","metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"a7b63639-89e1-45b2-8e22-968a1c201f41"},"useSeparator":"disabled"},{"content":{"params":{"text":"<h3> </h3>\n\n<h3><span style=\"font-size:1em\">Variety Selection</span></h3>\n\n<p>The following are the recommended varieties of squash:</p>\n\n<p> </p>\n\n<p><strong>1. Suprema F1</strong></p>\n"},"library":"H5P.AdvancedText 1.1","metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"8cf56008-a23c-4008-8e63-144e660bc249"},"useSeparator":"disabled"},{"content":{"params":{"contentName":"Image","alt":"Suprema f1 Variety","file":{"path":"images/image-lG4f0uG9.png","mime":"image/png","copyright":{"license":"U"},"width":1748,"height":1240}},"library":"H5P.Image 1.1","metadata":{"contentType":"Image","license":"U","title":"Untitled Image","authors":[],"changes":[]},"subContentId":"154cf8c4-8531-41c8-8a10-f1729fb0e268"},"useSeparator":"auto"},{"content":{"params":{"text":"<ul>\n\t<li>A high yielding and strong hybrid variety</li>\n\t<li>\n\t<p>Very popular and easy to grow</p>\n\t</li>\n\t<li>\n\t<p>Has highly preferred fruit quality</p>\n\t</li>\n</ul>\n\n<p> </p>\n\n<p><strong>2. Gracia F1</strong></p>\n"},"library":"H5P.AdvancedText 1.1","metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"64420201-72a9-4c96-9e5c-3d0433c5f2a8"},"useSeparator":"disabled"},{"content":{"params":{"contentName":"Image","file":{"path":"images/image-NN9ZXPgL.png","mime":"image/png","copyright":{"license":"U"},"width":1748,"height":1240},"alt":"Gracia F1 Variety"},"library":"H5P.Image 1.1","metadata":{"contentType":"Image","license":"U","title":"Untitled Image","authors":[],"changes":[]},"subContentId":"641948a5-42c8-4d21-9421-1c4451a18df5"},"useSeparator":"auto"},{"content":{"params":{"text":"<ul>\n\t<li>Has good disease and virus tolerance</li>\n\t<li>\n\t<p>Very early and fruits average weight is 3-4 kilograms</p>\n\t</li>\n\t<li>\n\t<p>Fruit has thick flesh, sticky and with attractive yellow color</p>\n\t</li>\n</ul>\n\n<p> </p>\n\n<p><strong>3. Matabia of Ilocos</strong></p>\n"},"library":"H5P.AdvancedText 1.1","metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"57192e2e-03fe-4b96-8650-833dbba3457f"},"useSeparator":"disabled"},{"content":{"params":{"contentName":"Image","file":{"path":"images/image-WOxu6wjh.png","mime":"image/png","copyright":{"license":"U"},"width":1748,"height":1240},"alt":"Matabia of Ilocos"},"library":"H5P.Image 1.1","metadata":{"contentType":"Image","license":"U","title":"Untitled Image","authors":[],"changes":[]},"subContentId":"59df2497-ca75-4687-976e-833bc4cdc20f"},"useSeparator":"disabled"},{"content":{"params":{"text":"<ul>\n\t<li>A native Ilocos variety of squash</li>\n\t<li>\n\t<p>Very early and fruits average weight is 1-2 kilograms</p>\n\t</li>\n\t<li>\n\t<p>Fruit has thin flesh, very sticky and with attractive color.</p>\n\t</li>\n</ul>\n"},"library":"H5P.AdvancedText 1.1","metadata":{"contentType":"Text","license":"U","title":"Untitled Text","authors":[],"changes":[]},"subContentId":"218d202d-a54f-4fee-97dd-328134cd6558"},"useSeparator":"disabled"}]}